<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>dioperations</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>JsonServlet</servlet-name>
    <servlet-class>com.pentaho.servlet.JsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JsonServlet</servlet-name>
    <url-pattern>/JsonServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Script</servlet-name>
    <servlet-class>com.pentaho.servlet.Script</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Script</servlet-name>
    <url-pattern>/Script/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NewEntryServlet</servlet-name>
    <servlet-class>com.pentaho.servlet.NewEntryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewEntryServlet</servlet-name>
    <url-pattern>/NewEntryServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddNewJob</servlet-name>
    <servlet-class>com.pentaho.servlet.AddNewJob</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddNewJob</servlet-name>
    <url-pattern>/AddNewJob/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddLoadSubLoadJob</servlet-name>
    <servlet-class>com.pentaho.servlet.AddLoadSubLoadJob</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddLoadSubLoadJob</servlet-name>
    <url-pattern>/AddLoadSubLoadJob/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddNewSubLoadJobName</servlet-name>
    <servlet-class>com.pentaho.servlet.AddNewSubLoadJobName</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddNewSubLoadJobName</servlet-name>
    <url-pattern>/AddNewSubLoadJobName/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.pentaho.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.pentaho.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet/*</url-pattern>
  </servlet-mapping>
</web-app>